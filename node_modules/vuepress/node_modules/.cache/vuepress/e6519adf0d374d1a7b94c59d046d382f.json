nkpreview.net\", keyVal).then(resp => {\n        console.log(resp.data);\n        // // SAVE\n        const localURL = \"http://localhost:3000/api/post\";\n        const saveURL = \"https://expressone.herokuapp.com/api/post\";\n        axios.post(saveURL, {\n          text: resp.data,\n          createdAt: new Date()\n        })\n          .then(response => {\n            axios.get(saveURL)\n              .then(update => {\n                console.log(update);\n              });\n          })\n          .catch(function (error) {\n            console.log(error);\n          });\n      });\n    }\n  }\n}\n</script>\n\n<style scoped>\n.urlinput {\n  width: 100%;\n  height: 30px;\n}\n\n.urlbutton {\n  width: 100%;\n  height: 30px;\n}\n</style>\n"]}]}